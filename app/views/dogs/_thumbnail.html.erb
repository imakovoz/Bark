<a href="<%= url_for(dog) %>">
  <article>
    <h2 class="dog-name"><%= dog.name %></h2>
    <%= image_tag url_for(dog.images.first), class: "dog-photo", alt: "Photo of #{dog.name}" %>
  </article>
</a>
<%= button_to "Like", {:controller => :likes,
  :action => 'create', dog_id: dog.id }, :method => :create %>

