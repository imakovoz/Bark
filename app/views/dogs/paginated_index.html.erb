<%= render partial: 'thumbnail', collection: @dogs, as: :dog %>
<% if @prev_page %>
    <%= link_to "Previous Page", controller: "dogs", action: "paginated_index", page: @page - 1 %>
<% end %>
<% if @next_page%>
    <%= link_to "Next Page", controller: "dogs", action: "paginated_index", page: @page + 1 %>
<% end %>